<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation=
               "http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="coffee1" class="model.impl.Coffee">
        <constructor-arg name="code" value="${code1}"/>
        <constructor-arg name="description" value="${description1}"/>
        <constructor-arg name="price" value="${price1}"/>
        <constructor-arg name="number" value="${number1}"/>
        <constructor-arg name="originCountry" value="${originCountry1}"/>
        <constructor-arg name="roast" value="${roast1}"/>
        <constructor-arg name="flavor" value="${flavor1}"/>
        <constructor-arg name="aroma" value="${aroma1}"/>
        <constructor-arg name="acidity" value="${acidity1}"/>
        <constructor-arg name="body" value="${body1}"/>
    </bean>

