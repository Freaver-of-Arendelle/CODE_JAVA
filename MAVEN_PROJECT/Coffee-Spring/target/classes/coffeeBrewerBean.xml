<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation=
               "http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="coffeeBrewer1" class="model.impl.CoffeeBrewer">
        <constructor-arg name="code" value="${brewerCode1}"/>
        <constructor-arg name="description" value="${brewerDescription1}"/>
        <constructor-arg name="price" value="${brewerPrice1}"/>
        <constructor-arg name="number" value="${brewerNumber1}"/>
        <constructor-arg name="brewerModel" value="${brewerModel1}"/>
        <constructor-arg name="waterSupply" value="${waterSupply1}"/>
        <constructor-arg name="cupsNumbers" value="${cupsNumbers1}"/>
    </bean>

